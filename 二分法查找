Position BinarySearch(List L,ElementType X){
    int Left,Right,Mid;
    if(L == NULL){
        return NotFound;
    }
    Left = 1;
    Right = L->Last;
    Mid = (Left + Right)/2;
    while(Left <= Right){
        if(X > L->Data[Mid]){
            Left = Mid + 1;
        }else if(X < L->Data[Mid]){
            Right = Mid - 1;
        }else if(X == L->Data[Mid]) {
            return Mid;
        }
        Mid = (Left + Right)/2;
        
    }
    return NotFound;
}
